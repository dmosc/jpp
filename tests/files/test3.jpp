import("libs/io.jpp")

class Point {
    var int x, y;

    construct(int _x, int _y) {
        this.x = _x;
        this.y = _y;
    }

    destruct() {
        write("Goodbye!");
    }

    func int getX() {
        return this.x;
    }

    func int getY() {
        return this.y;
    }
}

program TEST_3 {
    var Point p1, p2;
    p1 = Point(1, 2);
    write(p1.getX());
}