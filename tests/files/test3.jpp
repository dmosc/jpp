import("libs/io.jpp")

class Point {
    var int x, y;

    construct(int _x, int _y) {
        this.x = _x;
        this.y = _y;
    }

    destruct() {
        write("Goodbye!");
    }

    func int getX() {
        return x;
    }

    func int getY() {
        return y;
    }
}

program TEST_3 {
    var Point p1;
    var Point p2;
    p1 = new Point(1, 3);
    p2 = new Point(1, p1.getX());
    write(p1.getX());
}