import("libs/io.jpp")

class Point {
    var int x, y;

    construct(int a, int b) {
        this.x = a;
        this.y = b;
    }

    func int getX() {
        return this.x;
    }

    func int getY() {
        return this.y;
    }
}

program TEST_3 {
    var Point p1;
    p1 = new Point(1, 3);
    write("x: " + p1.getX());
    write("y: " + p1.getY());
}