import("libs/io.jpp")

class Point {
    var int x, y;

    construct(int a, int b) {
        this.x = a;
        this.y = b;
    }

    func int getX() {
        return this.x;
    }

    func int getY() {
        return this.y;
    }
}

program TEST_3 {
    var Point p1;
    write("Crating point p1");
    p1 = new Point(1, 3);
    write("p1(x): " + p1.getX());
    write("p1(y): " + p1.getY());

    write("----");
    write("Crating point p2");
    var Point p2;
    p2 = new Point(5, 8);
    write("p2(x): " + p2.getX());
    write("p2(y): " + p2.getY());
    
    write("----");
    write("Verifying p1 hasn't changed");
    write("p1(x): " + p1.getX());
    write("p1(y): " + p1.getY());

}