import("libs/io.jpp")

class Person {
    var string name;
}

func void setName(Person person, string name) {
    person.name = name;
}

program TEST_3 {
    var Person pato;
    pato = new Person();
    pato.name = "Pato";
   
    write(pato.name);

    setName(pato, "new name");

    write(pato.name);
}