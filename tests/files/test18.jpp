import("libs/io.jpp")

class Person {
    var string name;
    var int age;
    var Person friend;
}

program TEST_3 {
    var Person pato;
    pato = new Person();
    pato.name = "Pato";
    pato.age = 22;
    
    pato.friend = new Person();
